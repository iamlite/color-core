{
    "$schema": "https://typedoc.org/schema.json",

    "entryPoints": ["src"],

    "out": "docs",
    "lang": "en",
    "hideGenerator": true,
    "includeVersion": false,
    "highlightLanguages": ["js", "ts", "jsx", "tsx", "bash"],
    "plugin": ["./doctheme/cjs/index.js"],
    "theme": "dumi",
    "navigationLinks": {
        "GitHub": "https://github.com/iamlite/next-colors",
        "NPM": "https://www.npmjs.com/package/next-colors",
        "Demo Site": "https://next-colors-demo.vercel.app"
    },
    "navigationLinkTargets": {
        "GitHub": "_blank",
        "NPM": "_blank",
        "Demo Site": "_blank"
    },
    "logoImage": "https://typedoc.org/images/logo-32.png",
    "logoTitle": "Next-Colors",
    "footerAlign": "center"
}
